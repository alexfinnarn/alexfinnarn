<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}
        <link rel="stylesheet" href="https://highlightjs.org/static/demo/styles/stackoverflow-dark.css">

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body>
        <header>
            {{ include('_header.html.twig') }}
        </header>
        <main>
            {% block body %}{% endblock %}
        </main>
        <footer>
            2021c
        </footer>
    </body>
</html>
